 edtmax      = 0.80,
 edtmax_ocn  = 0.80,
 edtmino     = 0.20,
 edtmino_ocn = 0.20,
 edtmaxo     = 0.80,
 edtmaxo_ocn = 0.80,
 edtminx     = 0.20,
 edtminx_ocn = 0.20,
 edtmaxx     = 0.80,
 edtmaxx_ocn = 0.80,
 shrmin      = 0.30,
 shrmin_ocn  = 0.30,
 shrmax      = 0.90,
 shrmax_ocn  = 0.90,
 pbcmax = 50.0,
 mincld = 150.0,
 htmin = -250.0,
 htmax = 500.0,
 skbmax = 0.4,
 dtauc = 30.0,
 /

 &emanparam
 minsig = 0.95,
 elcrit_ocn = 0.0011,
 elcrit_lnd = 0.0011,
 tlcrit = -55.0,
 entp = 1.5,
 sigd = 0.05,
 sigs = 0.12,
