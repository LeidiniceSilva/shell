 budget_compute = .false.,
 nssopt = 1,
 iautoconv = 4,
 vfqr = 4.0,
 vfqi = 0.15,
 vfqs = 1.0,
 auto_rate_khair = 0.355,
 auto_rate_kessl = 1.e-3,
 auto_rate_klepi = 0.5e-3,
 rkconv = 1.666e-4,
 skconv = 1.0-3,
 rcovpmin = 0.1,
 rpecons = 5.547e-5,
 /

! 
! Parameters of cumulus (Grell, Emanuel, Tiedtke, Kain-Fritsch)
! 
