!
! This README details namelist parameters for RegCM Version 4.7
!
!---------------------------------------------------------------------------------
! Setting RegCM namelist parameters
! This namelist processes the variables defined in run_sub_regcm_clm45_cluster.sh
!
! Author: Leidinice Silva (leidinicesilva@gmail.com)
! Data: December 5, 2019
!
! In 17out2015: IInclusion of parameterization of variables &dimparam e &geoparam;
! In 29dez2016: IInclusion of the variable idyamic (hydros/non-hydros);
! In 10jan2017: IStart of global data testing using CFS;
! In 03out2018: Version 4.7 included new parameters. 
!
! Atention: parametro a seguir para indicar versao regcm.in
! Version: 4.7.1
!---------------------------------------------------------------------------------

!
! Dimension parameter of simulation
!
 &dimparam 
 iy     = 160,
 jx     = 320,
 kz     = 18,
 dsmin  = 0.01,
 dsmax  = 0.05,
 nsg    = 1,
 njxcpus = -1,
 niycpus = -1,
 /
 
!
! Dynamic core of the model 
!
 &coreparam
 idynamic = 2,  ! 1 = MM4 hydrostatical core
                ! 2 = MM5 NON hydrostatical core
 /

!
! Dynamical core parameters
! 
 &dynparam
 gnu1 = 0.1,
 gnu2 = 0.1,
 ckh = 1.0,
 adyndif = 1.0,
 diffu_hgtf = 0,
 upstream_mode = .true.,
 uoffc = 0.250,
 stability_enhance = .true.,
 vert_stability_enhance = .true.,
 t_extrema = 5.0,
 q_rel_extrema = 0.2,
 c_rel_extrema = 0.5,
 t_rel_extrema = 0.5,
 /

!
! Geolocation parameter of simulation
!
 &geoparam
 iproj = 'NORMER',
 ds = 25.0,
 ptop = 5.0,
 clat = -6.00,
 clon = -50.00,
 plat = -6.00,
 plon = -50.00,
 truelatl = 30.0,
 truelath = 60.0,
 i_band = 0,
 i_crm = 0,
 /
 
!
! Surface parameters of simulation
!
 &terrainparam
 domname = 'amz_neb',
 lresamp = .false.,
 smthbdy = .false.,
 ismthlev = 0,
 roidem = 1.5,
 h2ohgt = .true.,
 h2opct = 50.0,
 lakedpth = .false.,
 lsmoist  = .false.,
 fudge_lnd   = .false.,
 fudge_lnd_s = .false.,
 fudge_tex   = .false.,
 fudge_tex_s = .false.,
 fudge_lak   = .false.,
 fudge_lak_s = .false., 
 dirter = '/users/nice/RegCM_CLM/exp_doc/input',     
 inpter = '/vol3/disco1/nice/data_file/cmip_data/cmip5/hadgem2-es_downscaling',
 tersrc = 'GMTED',
 smsrc  = 'ESACCI',
 moist_filename = 'moist.nc',
 /

 &debugparam
 debug_level = 0,
 dbgfrq = 3,
 /

 &boundaryparam
 nspgx  = 12,
 nspgd  = 12,
 /
 
!
! ICBC Global data input control
! GLOBAL DATA AVAILABLE FROM ICTP FROM: http://clima-dods.ictp.it/Data/RegCM_Data
!
 &globdatparam
 ibdyfrq = 12,
 ssttyp = 'HA_85',
 dattyp = 'HA_85',
 chemtyp = 'MZCLM',
 gdate1 = 2004030100,
 gdate2 = 2004031000,
 calendar = '360_day', 
 dirglob = '/users/nice/RegCM_CLM/exp_doc/input',
 inpglob = '/vol3/disco1/nice/data_file/cmip_data/cmip5/hadgem2-es_downscaling',
 ensemble_run = .false.,
 /

 &globwindow
 lat0 = 0.0
 lat1 = 0.0
 lon0 = 0.0
 lon1 = 0.0
 /

! 
! This parameter control of the time period the model is currently simulating
! 
 &restartparam
 ifrest  = .false. ,
 mdate0  = 2004030100,
 mdate1  = 2004030100,
 mdate2  = 2004031000,
 /

! 
! This parameters contains model internal timesteps
! 
 &timeparam
 dtrad   =    30.,
 dtabem  =    18.,
 dtsrf   =   300.,
 dt      =    60.,
 /

! 
! This namelist controls the model output engine
! 
 &outparam
 ifsave  = .false., ! If save == true, create file for restart
 savfrq  =     0.,
 ifatm   = .true.,  ! If var == true, write file for restart 
 atmfrq  =     6.,  ! Frequency in hours of the var
 ifrad   = .false. 
 radfrq  =     6.,
 ifsts   = .true.,
 ifsrf   = .true.,
 srffrq  =     6.,
 ifsub   = .true.,
 subfrq  =     6.,
 iflak   = .false.,
 lakfrq  =     6.,
 ifchem  = .false.,
 ifopt   = .false.,
 chemfrq =     6.,
 enable_atm_vars = 73*.true.,
 enable_srf_vars = 44*.true.,
 enable_rad_vars = 28*.true.,
 enable_sub_vars = 18*.true.,
 enable_sts_vars = 18*.true.,
 enable_lak_vars = 18*.true.,
 enable_opt_vars = 20*.true.,
 enable_che_vars = 26*.true.,
 dirout  = '/users/nice/RegCM_CLM/exp_doc/output',
 lsync   = .false.,
 icosp = 0,
 idiag = 0,
 do_parallel_netcdf_in  = .false.,
 do_parallel_netcdf_out = .false.,
 /

! 
! This namelist controls the model physics
! 
 &physicsparam
 iboudy  =          5,  ! Lateral Boundary conditions scheme
                        ! 5 => Relaxation, exponential technique.
 isladvec =         0,  ! Semilagrangian advection scheme for tracers and
                        ! humidity
                        !   0 => Disabled
 iqmsl =            1,  ! Quasi-monotonic Semi Lagrangian
                        !   1 => Bermejo and Staniforth 1992 QMSL scheme
 ibltyp  =          1,  ! Boundary layer scheme
                        !   1 => Holtslag PBL (Holtslag, 1990)
 icup_lnd =         6,  ! Cumulus convection scheme Over Land
 icup_ocn =         6,  ! Cumulus convection scheme Over Icean
                        !   4 => Emanuel (1990), Kain (2004)
                        !   6 => Kain-Fritsch (1990), Kain (2004)
 ipptls  =          1,  ! Moisture scheme
                        !   1 => Explicit moisture (SUBEX; Pal et al 2000)
 iocncpl =          0,  ! Ocean SST from coupled Ocean Model through RegESM
                        !   1 => Coupling activated
 iwavcpl =          0,  ! Ocean roughness from coupled Wave Model through RegESM
                        !   1 => Coupling activated
 iocnflx =          2,  ! Ocean Flux scheme
                        !   2 => Zeng et al (1998)
 iocnrough =        1,  ! Zeng Ocean model roughness formula to use.
                        !   1 => (0.0065*ustar*ustar)/egrav
 iocnzoq =          1,  ! Zeng Ocean model factors for t,q roughness
                        !   1 => 2.67*(re**d_rfour) - 2.57
 ipgf    =          0,  ! Pressure gradient force scheme
                        !   0 => Use full fields
 iemiss  =          0,  ! Use computed long wave emissivity
 lakemod =          0,  ! Use lake model
 ichem   =          0,  ! Use active aerosol chemical model
 scenario =   'RCP8.5', !
 ghg_year_const = 1950, !
 idcsst   =          0, ! Use diurnal cycle sst scheme
 ipcpcool =          0, !
 iwhitecap =         0, !
 iseaice  =          0, ! Model seaice effects
 idesseas =          0, ! Model desert seasonal albedo variability
 iconvlwp =          1, ! Use convective algo for lwp in the large-scale
                        ! This is reset to zero if using ipptls = 2
 icldfrac =          0, ! Cloud fraction algorithm
                        !   0 : Original SUBEX
 icldmstrat =        1, ! Simulate stratocumulus clouds
 icumcloud =         1, ! Formulas to use for cumulus clouds (cf and lwc)
                        ! Cloud fractions, only if mass fluxes are not
                        ! available (Kuo and BM):
                        !   0,1 => cf = 1-(1-clfrcv)**(1/kdepth)
                        ! Liquid water content:
                        !   1,2 => function of temperature
 irrtm    =          0, ! Use RRTM radiation scheme instead of CCSM
 iclimao3 =          0, !
 iclimaaer =         0, !
 isolconst =         0, !
 islab_ocean =       0, !
 itweak =            0, ! Enable tweak scenario
 ifixsolar =         0, !
 fixedsolarval =  343., !
 /

 &referenceatm
 base_state_pressure = 101325.0,
 logp_lrate = 47.70,
 /

! 
! This namelist controls non-hydrostatic core 
! 
 &nonhydroparam
 ifupr = 1,
 nhbet = 0.4,
 nhxkd = 0.1,
 ifrayd = 1,
 rayndamp = 5,
 rayalpha0 = 0.001,
 rayhd = 10000.0,
 /

 &hydroparam
 nsplit = 2,
 lstand = .true.,
 /

! 
! This namelist controls the new microphysics scheme
! 
&boundaryparam
 nspgx  = 12,
 nspgd  = 12,
 high_nudge =    3.0,
 medium_nudge =  2.0,
 low_nudge =     1.0,
 bdy_nm = -1.0,
 bdy_dm = -1.0,
/
 
 &cldparam
 ncld      = 1,
 rhmax     = 1.01,
 rhmin     = 0.01,
 rh0land   = 0.80,
 rh0oce    = 0.90,
 tc0       = 238.0,
 cllwcv    = 0.3e-3,
 clfrcvmax = 0.75,
 cftotmax  = 0.75,
 kfac_shal = 0.07,
 kfac_deep = 0.14,
 k2_const  = 500.0,
 lsrfhack  = .false.,
 larcticcorr = .true.,
 rcrit     = 13.5,
 coef_ccn  = 2.5e+20,
 abulk     = 0.9,
 /

! 
! This namelist controls the SUBEX moisture scheme
! 
 &subexparam
 qck1land  = 0.0005,
 qck1oce   = 0.0005,
 gulland   = 0.65,
 guloce    = 0.30,
 cevaplnd  = 1.0e-5,
 cevapoce  = 1.0e-5,
 caccrlnd  = 6.0,
 caccroce  = 4.0,
 conf      = 1.00,
 /

! 
! This namelist controls the new microphysics scheme
! 
 &microparam
